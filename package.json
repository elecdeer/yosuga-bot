{
  "name": "yosuga-bot",
  "version": "1.2.4",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:live",
    "dev": "ts-node --files src/index.ts",
    "dev:watch": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "test": "jest",
    "check": "npm-run-all check:*",
    "check:type": "tsc",
    "check:format": "prettier --check 'src/**/*.ts' 'src/*.ts'",
    "check:lint": "eslint 'src/**/*.ts' 'src/*.ts' --max-warnings 0",
    "format": "prettier 'src/**/*.ts' 'src/*.ts' --write",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier src/**/*.ts src/*.ts --write",
      "eslint src/**/*.ts src/*.ts --max-warnings 0"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": "18.x"
  },
  "devDependencies": {
    "@types/async": "^3.2.13",
    "@types/charset": "^1.0.2",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.44",
    "@types/open-graph-scraper": "^4.8.1",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.1",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "type-fest": "^2.13.1",
    "typescript": "4.7.3"
  },
  "dependencies": {
    "@aspida/axios": "^1.10.2",
    "@discordjs/opus": "^0.7.0",
    "@discordjs/rest": "^0.5.0",
    "@discordjs/voice": "0.10.0",
    "@elecdeer/emoji-pronunciation-ja": "^1.0.1",
    "@kvs/node-localstorage": "^2.1.1",
    "@kvs/types": "^2.1.1",
    "@sap_oss/node-socketio-stream": "^1.0.3",
    "async": "^3.2.4",
    "axios": "^0.27.2",
    "bufferutil": "^4.0.6",
    "charset": "^1.0.1",
    "deepmerge": "^4.2.2",
    "discord-api-types": "^0.34.0",
    "discord.js": "^13.8.0",
    "dotenv": "^16.0.1",
    "emoji-regex": "^10.1.0",
    "eslint-plugin-import": "^2.26.0",
    "ffmpeg-static": "^4.4.1",
    "http-status": "^1.5.2",
    "log4js": "^6.5.2",
    "open-graph-scraper": "^4.11.0",
    "socket.io-client": "^4.5.1",
    "sodium-native": "^3.3.0",
    "url-regex-safe": "^3.0.0",
    "utf-8-validate": "^5.0.9"
  },
  "eslintIgnore": [
    "/.eslintrc.js"
  ]
}
