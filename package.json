{
  "name": "yosuga-bot",
  "version": "1.2.4",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "node --enable-source-maps dist/index.mjs",
    "build": "rimraf dist && rollup -c",
    "dev": "rimraf dist && rollup -c --environment DEV",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "check": "npm-run-all check:*",
    "check:type": "tsc",
    "check:format": "prettier --cache --check 'src/**/*.ts' 'src/*.ts'",
    "check:lint": "eslint --cache 'src/**/*.ts' 'src/*.ts' --max-warnings 0",
    "format": "prettier --cache  'src/**/*.ts' 'src/*.ts' --write",
    "prepare": "node -e \"if(process.env.NODE_ENV !== 'production'){ console.log('install husky'); require('husky').install() }\""
  },
  "keywords": [],
  "author": "elecdeer",
  "license": "MIT",
  "engines": {
    "node": "18.x"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-run": "^2.1.0",
    "@types/async": "^3.2.15",
    "@types/charset": "^1.0.2",
    "@types/node": "^18.7.14",
    "@types/object-hash": "^2.2.1",
    "@types/open-graph-scraper": "^4.8.1",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitest/ui": "^0.22.1",
    "esbuild": "^0.15.6",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-esbuild": "^4.10.1",
    "type-fest": "^2.19.0",
    "typescript": "4.8.2",
    "vitest": "^0.22.1"
  },
  "dependencies": {
    "@aspida/axios": "^1.11.0",
    "@discordjs/opus": "^0.8.0",
    "@discordjs/rest": "^1.1.0",
    "@discordjs/voice": "0.11.0",
    "@elecdeer/emoji-pronunciation-ja": "^1.0.1",
    "@prisma/client": "^4.3.0",
    "@sap_oss/node-socketio-stream": "^1.0.3",
    "async": "^3.2.4",
    "axios": "^0.27.2",
    "bufferutil": "^4.0.6",
    "charset": "^1.0.1",
    "discord-api-types": "^0.37.5",
    "discord.js": "^14.3.0",
    "dotenv": "^16.0.2",
    "emoji-regex": "^10.1.0",
    "ffmpeg-static": "^5.1.0",
    "http-status": "^1.5.2",
    "husky": "^8.0.1",
    "log4js": "^6.6.1",
    "object-hash": "^3.0.0",
    "open-graph-scraper": "^4.11.1",
    "prisma": "^4.3.1",
    "socket.io-client": "^4.5.1",
    "sodium-native": "^3.4.1",
    "utf-8-validate": "^5.0.9",
