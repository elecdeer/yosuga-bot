{
  "name": "yosuga-bot",
  "version": "1.2.4",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:live",
    "dev": "ts-node --files src/index.ts",
    "dev:watch": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "test": "jest",
    "check": "npm-run-all check:*",
    "check:type": "tsc",
    "check:format": "prettier --check src/**/*.ts src/*.ts",
    "check:lint": "eslint src/**/*.ts src/*.ts --max-warnings 0",
    "format": "prettier src/**/*.ts src/*.ts --write",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier src/**/*.ts src/*.ts --write",
      "eslint src/**/*.ts src/*.ts --max-warnings 0"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": "16.x"
  },
  "devDependencies": {
    "@types/async": "^3.2.12",
    "@types/charset": "^1.0.2",
    "@types/encoding-japanese": "^1.0.18",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/open-graph-scraper": "^4.8.1",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "eslint-import-resolver-typescript": "^2.7.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "4.6.2"
  },
  "dependencies": {
    "@aspida/axios": "^1.9.0",
    "@discordjs/opus": "^0.7.0",
    "@discordjs/rest": "^0.3.0",
    "@discordjs/voice": "0.8.0",
    "@elecdeer/emoji-pronunciation-ja": "^1.0.1",
    "@kvs/node-localstorage": "^1.2.0",
    "@kvs/types": "^1.2.0",
    "@sap_oss/node-socketio-stream": "^1.0.3",
    "async": "^3.2.3",
    "axios": "^0.26.1",
    "bufferutil": "^4.0.6",
    "charset": "^1.0.1",
    "deepmerge": "^4.2.2",
    "discord-api-types": "^0.29.0",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "emoji-regex": "^10.0.1",
    "eslint-plugin-import": "^2.25.4",
    "ffmpeg-static": "^4.4.1",
    "http-status": "^1.5.0",
    "husky": "^7.0.4",
    "libsodium-wrappers": "^0.7.10",
    "lint-staged": "^12.3.7",
    "log4js": "^6.4.2",
    "open-graph-scraper": "^4.11.0",
    "prism-media": "^1.3.2",
    "socket.io-client": "^4.4.1",
    "type-fest": "^2.12.0",
    "url-regex-safe": "^3.0.0",
    "utf-8-validate": "^5.0.9"
  },
  "eslintIgnore": [
    "/.eslintrc.js"
  ]
}
