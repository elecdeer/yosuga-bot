{
  "name": "yosuga-bot",
  "version": "1.2.4",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:live",
    "dev": "ts-node --files src/index.ts",
    "dev:watch": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "check": "npm-run-all check:*",
    "check:type": "tsc",
    "check:format": "prettier --check src/**/*.ts src/*.ts",
    "check:lint": "eslint src/**/*.ts src/*.ts --max-warnings 0",
    "format": "prettier src/**/*.ts src/*.ts --write",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier src/**/*.ts src/*.ts --write",
      "eslint src/**/*.ts src/*.ts --max-warnings 0"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": "16.x"
  },
  "devDependencies": {
    "@types/async": "^3.2.7",
    "@types/charset": "^1.0.2",
    "@types/encoding-japanese": "^1.0.18",
    "@types/node": "^16.7.10",
    "@types/open-graph-scraper": "^4.8.1",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@discordjs/builders": "^0.6.0",
    "@discordjs/opus": "^0.5.3",
    "@discordjs/voice": "0.6.0",
    "@elecdeer/emoji-pronunciation-ja": "^1.0.1",
    "@kvs/node-localstorage": "^1.2.0",
    "@kvs/types": "^1.2.0",
    "@sap_oss/node-socketio-stream": "^1.0.3",
    "async": "^3.2.1",
    "axios": "^0.21.1",
    "bufferutil": "^4.0.3",
    "charset": "^1.0.1",
    "deepmerge": "^4.2.2",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "emoji-regex": "^9.2.2",
    "eslint-plugin-import": "^2.24.2",
    "ffmpeg-static": "^4.4.0",
    "http-status": "^1.5.0",
    "husky": "^7.0.2",
    "libsodium-wrappers": "^0.7.9",
    "lint-staged": "^11.1.2",
    "log4js": "^6.3.0",
    "open-graph-scraper": "^4.9.2",
    "prism-media": "^1.3.2",
    "re2": "^1.16.0",
    "socket.io-client": "^4.2.0",
    "type-fest": "^2.1.0",
    "url-regex-safe": "^2.0.2",
    "utf-8-validate": "^5.0.5"
  },
  "eslintIgnore": [
    "/.eslintrc.js"
  ]
}
