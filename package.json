{
  "name": "yosuga-bot",
  "version": "1.2.4",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "vite-node src/index.ts",
    "dev": "vite-node src/index.ts",
    "dev:watch": "vite-node -w src/index.ts",
    "test": "vitest",
    "test:watch": "vitest watch",
    "check": "npm-run-all check:*",
    "check:type": "tsc",
    "check:format": "prettier --cache --check 'src/**/*.ts' 'src/*.ts'",
    "check:lint": "eslint 'src/**/*.ts' 'src/*.ts' --max-warnings 0",
    "format": "prettier 'src/**/*.ts' 'src/*.ts' --write",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier src/**/*.ts src/*.ts --write",
      "eslint src/**/*.ts src/*.ts --max-warnings 0"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": "18.x"
  },
  "devDependencies": {
    "@types/async": "^3.2.15",
    "@types/charset": "^1.0.2",
    "@types/node": "^18.7.2",
    "@types/open-graph-scraper": "^4.8.1",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vitest/ui": "^0.21.1",
    "eslint": "^8.21.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "type-fest": "^2.18.0",
    "typescript": "4.7.4",
    "vitest": "^0.21.1"
  },
  "dependencies": {
    "@aspida/axios": "^1.10.3",
    "@discordjs/opus": "^0.8.0",
    "@discordjs/rest": "^1.0.0",
    "@discordjs/voice": "0.11.0",
    "@elecdeer/emoji-pronunciation-ja": "^1.0.1",
    "@prisma/client": "^4.2.1",
    "@sap_oss/node-socketio-stream": "^1.0.3",
    "async": "^3.2.4",
    "axios": "^0.27.2",
    "bufferutil": "^4.0.6",
    "charset": "^1.0.1",
    "discord-api-types": "^0.37.1",
    "discord.js": "^14.2.0",
    "dotenv": "^16.0.1",
    "emoji-regex": "^10.1.0",
    "eslint-plugin-import": "^2.26.0",
    "ffmpeg-static": "^5.0.2",
    "http-status": "^1.5.2",
    "log4js": "^6.6.1",
    "open-graph-scraper": "^4.11.1",
    "prisma": "^4.2.1",
    "socket.io-client": "^4.5.1",
    "sodium-native": "^3.3.0",
    "utf-8-validate": "^5.0.9",
    "vite-node": "^0.21.1",
    "zod": "^3.18.0"
  },
  "eslintIgnore": [
    "/.eslintrc.cjs"
  ]
}
