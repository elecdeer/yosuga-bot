{
  "name": "yosuga-bot",
  "version": "1.2.4",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "vite-node src/index.ts",
    "dev": "vite-node src/index.ts",
    "dev:watch": "vite-node -w src/index.ts",
    "test": "vitest",
    "test:watch": "vitest watch",
    "check": "npm-run-all check:*",
    "check:type": "tsc",
    "check:format": "prettier --cache --check 'src/**/*.ts' 'src/*.ts'",
    "check:lint": "eslint 'src/**/*.ts' 'src/*.ts' --max-warnings 0",
    "format": "prettier 'src/**/*.ts' 'src/*.ts' --write",
    "prepare": "node -e \"if(process.env.NODE_ENV !== 'production'){ console.log('install husky'); require('husky').install() }\"",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier src/**/*.ts src/*.ts --write",
      "eslint src/**/*.ts src/*.ts --max-warnings 0"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": "18.x"
  },
  "devDependencies": {
    "@types/async": "^3.2.16",
    "@types/charset": "^1.0.2",
    "@types/node": "^18.11.18",
    "@types/open-graph-scraper": "^4.8.2",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vitest/ui": "^0.28.3",
    "eslint": "^8.33.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "type-fest": "^3.5.4",
    "typescript": "4.9.5",
    "vitest": "^0.28.3"
  },
  "dependencies": {
    "@aspida/axios": "^1.11.0",
    "@discordjs/opus": "^0.9.0",
    "@discordjs/rest": "^1.5.0",
    "@discordjs/voice": "^0.14.0",
    "@elecdeer/emoji-pronunciation-ja": "^1.0.1",
    "@kvs/node-localstorage": "^2.1.1",
    "@kvs/types": "^2.1.1",
    "@sap_oss/node-socketio-stream": "^1.0.3",
    "async": "^3.2.4",
    "axios": "^1.3.1",
    "bufferutil": "^4.0.7",
    "charset": "^1.0.1",
    "deepmerge": "^4.3.0",
    "discord-api-types": "^0.36.3",
    "discord.js": "^13.12.0",
    "dotenv": "^16.0.3",
    "emoji-regex": "^10.2.1",
    "eslint-plugin-import": "^2.27.5",
    "ffmpeg-static": "^5.1.0",
    "http-status": "^1.6.2",
    "log4js": "^6.7.1",
    "open-graph-scraper": "^4.11.1",
    "socket.io-client": "^4.5.4",
    "sodium-native": "^4.0.1",
    "url-regex-safe": "^3.0.0",
    "utf-8-validate": "^6.0.2",
    "vite-node": "^0.28.3"
  },
  "eslintIgnore": [
    "/.eslintrc.cjs"
  ]
}
